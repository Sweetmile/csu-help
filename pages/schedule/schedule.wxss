.head {
  padding: 0;
  height: 40px;
  text-align: center;
  background-color: #fff;
}
.flex-item {
  width: 95rpx;
  height: 100px;
}

.kcb-item {
  position: absolute;
  justify-content: center;
  display: flex;
  align-items: center;
  border-radius: 5px;
}

.smalltext {
  font-size: 8pt;
  color: #fff;
  padding-left: 2px;
}

.top {
  display: flex;
  flex-direction: row;
  padding-left: 35rpx;
  background-color: #fff;
  color: #7cba23;
  height: 22px;
}

.top-text {
  width: 100rpx;
  height: 40rpx;
  font-size: 12pt;
  justify-content: center;
  display: flex;
  align-items: center;
  border-bottom:1rpx solid lightgray;
}

.scroll {
  height: 1170rpx;
  z-index: 101;
  position: fixed;
  background-color: #fff;
}

.left {
  width: 35rpx;
  height: 120rpx;
  font-size: 9pt;
  justify-content: center;
  display: flex;
  align-items: center;
  border-right:1rpx solid lightgray;
}